# StakeTrack Development Environment Configuration

# Application Configuration
NODE_ENV=development
APP_VERSION=1.0.0
APP_ENVIRONMENT=DEVELOPMENT
BASE_URL=/

# Feature Flags
APP_ENABLE_ANALYTICS=false
APP_AI_ENABLED=true

# API Configuration
APP_API_BASE_URL=http://localhost:5001/staketrack-dev/europe-west1

# Firebase Configuration
# NOTE: Sensitive values (API keys, app IDs, etc.) should be stored in .env.development.local (not committed)
# Only non-sensitive values should be stored here
APP_FIREBASE_AUTH_DOMAIN=staketrack-dev.firebaseapp.com
APP_FIREBASE_PROJECT_ID=staketrack-dev
APP_FIREBASE_STORAGE_BUCKET=staketrack-dev.appspot.com
APP_FIREBASE_FUNCTIONS_REGION=europe-west1

# AI Configuration
APP_AI_MODEL=gemini-1.5-pro
APP_AI_WEEKLY_LIMIT=10 

# Development-specific Settings
APP_USE_EMULATORS=true
APP_EMULATOR_AUTH_PORT=9099
APP_EMULATOR_FIRESTORE_PORT=8080
APP_EMULATOR_FUNCTIONS_PORT=5001 