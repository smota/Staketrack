# StakeTrack Production Environment Configuration

# Application Configuration
NODE_ENV=production
VUE_APP_VERSION=1.0.0
VUE_APP_ENVIRONMENT=PRD

# Feature Flags
VUE_APP_USE_EMULATORS=false
VUE_APP_ENABLE_ANALYTICS=true

# API Configuration
VUE_APP_API_BASE_URL=https://api.staketrack.com

# No secrets here - these are loaded from Firebase Functions environment
# during deployment

# Firebase configuration for PRODUCTION environment

ENVIRONMENT=PRD

# Firebase Configuration
FIREBASE_API_KEY=your-prod-api-key
FIREBASE_AUTH_DOMAIN=your-prod-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-prod-project
FIREBASE_STORAGE_BUCKET=your-prod-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=67890
FIREBASE_APP_ID=1:67890:web:ghijkl
FIREBASE_MEASUREMENT_ID=G-YYYYYYY

# Production settings
USE_EMULATORS=false

# API Settings
ANTHROPIC_API_KEY=your-anthropic-api-key
ANTHROPIC_API_ENDPOINT=https://api.anthropic.com/v1/messages 