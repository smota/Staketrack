# Firebase configuration for PRODUCTION environment

ENVIRONMENT=PRD

# Firebase Configuration
FIREBASE_API_KEY=your-prod-api-key
FIREBASE_AUTH_DOMAIN=your-prod-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-prod-project
FIREBASE_STORAGE_BUCKET=your-prod-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=67890
FIREBASE_APP_ID=1:67890:web:ghijkl
FIREBASE_MEASUREMENT_ID=G-YYYYYYY

# Production settings
USE_EMULATORS=false

# API Settings
ANTHROPIC_API_KEY=your-anthropic-api-key
ANTHROPIC_API_ENDPOINT=https://api.anthropic.com/v1/messages 