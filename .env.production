# StakeTrack Production Environment Configuration

# Application Configuration
NODE_ENV=production
APP_VERSION=1.0.0
APP_ENVIRONMENT=PRODUCTION
BASE_URL=/

# Feature Flags
APP_ENABLE_ANALYTICS=true
APP_AI_ENABLED=true

# API Configuration
APP_API_BASE_URL=https://europe-west1-staketrack-prod.cloudfunctions.net

# Firebase Configuration
# NOTE: Sensitive values (API keys, app IDs, etc.) should be stored in .env.production.local (not committed)
# Only non-sensitive values should be stored here
APP_FIREBASE_AUTH_DOMAIN=staketrack-prod.firebaseapp.com
APP_FIREBASE_PROJECT_ID=staketrack-prod
APP_FIREBASE_STORAGE_BUCKET=staketrack-prod.appspot.com
APP_FIREBASE_FUNCTIONS_REGION=europe-west1

# AI Configuration
APP_AI_MODEL=gemini-1.5-pro
APP_AI_WEEKLY_LIMIT=10

# Production-specific Settings
APP_USE_EMULATORS=false