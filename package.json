{
  "name": "staketrack",
  "version": "1.0.0",
  "description": "StakeTrack application",
  "main": "index.html",
  "scripts": {
    "dev": "dotenv -e .env.development webpack-dev-server --mode development",
    "build": "dotenv -e .env.production webpack --mode production",
    "serve": "firebase serve --only hosting",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint js/",
    "lint:fix": "eslint js/ --fix",
    "build:dev": "dotenv -e .env.development webpack --mode development",
    "build:prod": "dotenv -e .env.production webpack --mode production",
    "firebase:setup": "node scripts/firebase-setup.js",
    "firebase:deploy": "node scripts/firebase-deploy.js",
    "firebase:update-config": "node scripts/update-firebase-config.js",
    "firebase:update-config:dev": "node scripts/update-firebase-config.js --env=dev",
    "firebase:update-config:prod": "node scripts/update-firebase-config.js --env=prd",
    "firebase:init-functions": "node scripts/init-firebase-functions.js",
    "firebase:init-functions:dev": "node scripts/init-firebase-functions.js --env=dev",
    "firebase:init-functions:prod": "node scripts/init-firebase-functions.js --env=prd",
    "deploy:dev": "node scripts/firebase-deploy.js --env=DEV --option=hosting",
    "deploy:prod": "node scripts/firebase-deploy.js --env=PRD --option=hosting",
    "deploy:functions:dev": "node scripts/deploy-functions.js development",
    "deploy:functions:prod": "node scripts/deploy-functions.js production",
    "deploy:rules:dev": "node scripts/firebase-deploy.js --env=DEV --option=rules",
    "deploy:rules:prod": "node scripts/firebase-deploy.js --env=PRD --option=rules",
    "deploy:all:dev": "node scripts/firebase-deploy.js --env=DEV --option=all",
    "deploy:all:prod": "node scripts/firebase-deploy.js --env=PRD --option=all",
    "deploy:config-function:dev": "node scripts/deploy-functions.js development",
    "deploy:config-function:prod": "node scripts/deploy-functions.js production",
    "emulators": "firebase emulators:start"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.23.9",
    "@testing-library/jest-dom": "^6.4.2",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "dotenv-cli": "^8.0.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@firebase/analytics": "^0.10.12",
    "@firebase/app": "^0.11.2",
    "@firebase/auth": "^1.9.1",
    "@firebase/firestore": "^4.7.9",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.200.0",
    "@opentelemetry/api-metrics": "^0.33.0",
    "@opentelemetry/context-zone": "^2.0.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.200.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.200.0",
    "@opentelemetry/instrumentation": "^0.200.0",
    "@opentelemetry/instrumentation-document-load": "^0.44.1",
    "@opentelemetry/instrumentation-fetch": "^0.200.0",
    "@opentelemetry/resources": "^2.0.0",
    "@opentelemetry/sdk-logs": "^0.200.0",
    "@opentelemetry/sdk-metrics": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-web": "^2.0.0",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "dotenv": "^16.4.5",
    "firebase": "^8.10.1",
    "process": "^0.11.10"
  }
}