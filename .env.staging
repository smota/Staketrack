# StakeTrack Staging Environment Configuration

# Application Configuration
NODE_ENV=production
APP_VERSION=1.0.0
APP_ENVIRONMENT=STAGING
BASE_URL=/

# Feature Flags
APP_USE_EMULATORS=false
APP_ENABLE_ANALYTICS=true

# API Configuration
APP_API_BASE_URL=https://europe-west1-staketrack-dev.cloudfunctions.net

# Firebase non-sensitive configuration
# Actual API keys and secrets should ONLY be in .env.staging.local (gitignored)
APP_FIREBASE_AUTH_DOMAIN=staketrack-dev.firebaseapp.com
APP_FIREBASE_PROJECT_ID=staketrack-dev
APP_FIREBASE_STORAGE_BUCKET=staketrack-dev.firebasestorage.app
APP_FIREBASE_FUNCTIONS_REGION=europe-west1

# AI Configuration - Non-sensitive
# Uses Firebase Vertex AI with Gemini models
APP_AI_MODEL=gemini-1.5-pro
APP_AI_ENABLED=true
APP_AI_WEEKLY_LIMIT=10

# Staging settings
APP_USE_EMULATORS=false

# API service endpoints (no secrets)
APP_ANTHROPIC_API_ENDPOINT=https://api.anthropic.com/v1/messages 